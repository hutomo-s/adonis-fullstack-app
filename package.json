{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
    "start": "node server.js",
    "test:ace": "node ace test",
    "test": "jest",
    "serve:dev": "nodemon --watch app --watch start --watch config --watch .env -x node server.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/vow": "^1.0.17",
    "@adonisjs/vow-browser": "^1.0.7",
    "html-minifier": "^4.0.0",
    "multimatch": "^4.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^24.8.0",
    "laravel-mix": "^4.0.0",
    "laravel-mix-imagemin": "^1.0.0",
    "ncp": "^2.0.0",
    "precss": "^4.0.0",
    "sass": "^1.20.3",
    "vue-template-compiler": "^2.6.10"
  },
  "autoload": {
    "App": "./app"
  }
}
